{
  "name": "webview-bun",
  "description": "Bun bindings for webview, a tiny library for creating web-based desktop GUIs.",
  "version": "2.4.0",
  "type": "module",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "files": [
    "src/",
    "build/"
  ],
  "scripts": {
    "build": "bun scripts/build.ts",
    "clean": "bun exec \"rm -rf webview/build\"",
    "pretty": "bunx --bun prettier . \"!webview/**\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nglmercer/webview-bun.git"
  },
  "keywords": [
    "bun",
    "webview",
    "webview-bun",
    "desktop",
    "gui",
    "ffi",
    "native"
  ],
  "author": "nglmercer",
  "license": "MIT",
  "contributors": [
    "citkane",
    "WebReflection",
    "StrangeBytesDev"
  ],
  "bugs": {
    "url": "https://github.com/nglmercer/webview-bun/issues"
  },
  "homepage": "https://github.com/nglmercer/webview-bun#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest"
  }
}
